{
  "template": [
    {
      "type": "card-template",
      "attr": {
        "entry": "notification",
        "category": "symmetry"
      },
      "children": [
        {
          "type": "image",
          "attr": {
            "level": "0:A*",
            "src": "assets/images/logo.png"
          }
        },
        {
          "type": "text",
          "attr": {
            "level": "1:B*",
            "value": "{{symmetry_additional}}"
          }
        },
        {
          "type": "div",
          "attr": {
            "level": "5:F",
            "bgImage": "assets/images/symmetry_bg.png"
          },
          "children": [
            {
              "type": "div",
              "attr": {
                "level": "6-1:G1",
                "bgImage": "assets/images/symmetry_bg.png"
              },
              "children": [
                {
                  "type": "image",
                  "attr": {
                    "level": "0-1:A1*",
                    "src": "assets/images/logo.png"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "2-1:C1",
                    "value": "{{symmetry_name}}"
                  }
                }
              ]
            },
            {
              "type": "text",
              "attr": {
                "level": "1-1:B1*",
                "value": "{{symmetry_content}}"
              }
            },
            {
              "type": "text",
              "attr": {
                "level": "2-2:C2*",
                "value": "{{$t('strings.symmetry_number')}}"
              }
            },
            {
              "type": "text",
              "attr": {
                "level": "2-3:C3",
                "value": "{{$t('strings.symmetry_checkIn')}}"
              }
            },
            {
              "type": "div",
              "attr": {
                "level": "6:G"
              },
              "children": [
                {
                  "type": "text",
                  "attr": {
                    "level": "2-4:C4",
                    "value": "{{symmetry_departureLocation}}"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "2-5:C5",
                    "value": "{{$t('strings.symmetry_destLocation')}}"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "3-1:D1",
                    "value": "{{symmetry_departureTime}}"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "3-2:D2",
                    "value": "{{symmetry_destTime}}"
                  }
                },
                {
                  "type": "image",
                  "attr": {
                    "level": "3-3:D3",
                    "src": "assets/images/transferring.png"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "3-4:D4",
                    "value": "{{symmetry_extraDays}}"
                  }
                }
              ]
            },
            {
              "type": "button",
              "attr": {
                "level": "4:E",
                "type": "capsule",
                "value": "{{symmetryBtnText}}"
              },
              "events": {
                "click": "clickButton"
              }
            }
          ]
        }
      ]
    },
    {
      "type": "card-template",
      "attr": {
        "entry": "desktop"
      },
      "children": [
        {
          "type": "div",
          "attr": {},
          "classList": [
            "container"
          ],
          "children": [
            {
              "type": "div",
              "attr": {},
              "classList": [
                "top"
              ],
              "children": [
                {
                  "type": "image",
                  "attr": {
                    "src": "assets/images/logo.png"
                  },
                  "classList": [
                    "icon"
                  ]
                },
                {
                  "type": "text",
                  "attr": {
                    "value": "{{$t('strings.title')}}"
                  },
                  "classList": [
                    "text",
                    "title"
                  ]
                }
              ]
            },
            {
              "type": "div",
              "attr": {},
              "classList": [
                "center"
              ],
              "children": [
                {
                  "type": "div",
                  "attr": {},
                  "classList": [
                    "bigCard"
                  ],
                  "children": [
                    {
                      "type": "div",
                      "attr": {},
                      "classList": [
                        "message"
                      ],
                      "events": {
                        "click": "clickScan"
                      },
                      "children": [
                        {
                          "type": "image",
                          "attr": {
                            "src": "assets/images/Scan.png"
                          },
                          "classList": [
                            "image"
                          ]
                        },
                        {
                          "type": "text",
                          "attr": {
                            "value": "{{$t('strings.describe')}}"
                          },
                          "classList": [
                            "text",
                            "describe"
                          ]
                        }
                      ]
                    },
                    {
                      "type": "div",
                      "attr": {},
                      "classList": [
                        "message"
                      ],
                      "events": {
                        "click": "clickQR"
                      },
                      "children": [
                        {
                          "type": "image",
                          "attr": {
                            "src": "assets/images/QR.png"
                          },
                          "classList": [
                            "image"
                          ]
                        },
                        {
                          "type": "text",
                          "attr": {
                            "value": "{{$t('strings.describe1')}}"
                          },
                          "classList": [
                            "text",
                            "describe"
                          ]
                        }
                      ]
                    },
                    {
                      "type": "div",
                      "attr": {},
                      "classList": [
                        "message"
                      ],
                      "events": {
                        "click": "clickNFC"
                      },
                      "children": [
                        {
                          "type": "image",
                          "attr": {
                            "src": "assets/images/nfc.png"
                          },
                          "classList": [
                            "image"
                          ]
                        },
                        {
                          "type": "text",
                          "attr": {
                            "value": "{{$t('strings.describe2')}}"
                          },
                          "classList": [
                            "text",
                            "describe"
                          ]
                        }
                      ]
                    },
                    {
                      "type": "div",
                      "attr": {},
                      "classList": [
                        "message"
                      ],
                      "events": {
                        "click": "clickTRANSFER"
                      },
                      "children": [
                        {
                          "type": "image",
                          "attr": {
                            "src": "assets/images/Frame 4.png"
                          },
                          "classList": [
                            "image"
                          ]
                        },
                        {
                          "type": "text",
                          "attr": {
                            "value": "{{$t('strings.describe3')}}"
                          },
                          "classList": [
                            "text",
                            "describe"
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "data": {
    "symmetry_logo": "assets/images/logo.png",
    "symmetry_content": "-RM 40.00",
    "symmetry_airplane": "assets/images/transferring.png",
    "symmetry_destTime": "+RM 40.00",
    "symmetry_extraDays": "",
    "symmetry_bg": "assets/images/symmetry_bg.png",
    "symmetry_departureLocation": "TEY HENRY",
    "symmetry_departureTime": "-RM 40.00",
    "symmetry_name": "OPPO Pay",
    "symmetryBtnText": "Report",
    "symmetry_additional": "$t('strings.symmetry_additional')"
  },
  "actions": {
    "clickScan": {
      "type": "deeplink",
      "uri": "com.seedling.demo.hostdeveloperdemo.ACTION_SCAN",
      "data": "uri",
      "params": {}
    },
    "clickQR": {
      "type": "deeplink",
      "uri": "nativeapp://com.seedling.demo.hostdeveloperdemo.ACTION_QR",
      "data": "uri",
      "params": {}
    },
    "clickNFC": {
      "type": "deeplink",
      "uri": "nativeapp://com.seedling.demo.hostdeveloperdemo.ACTION_NFC",
      "data": "uri",
      "params": {}
    },
    "clickTRANSFER": {
      "type": "deeplink",
      "uri": "nativeapp://com.seedling.demo.hostdeveloperdemo.ACTION_TRANSFER",
      "data": "uri",
      "params": {}
    }
  },
  "styles": {
    ".container": {
      "width": "100%",
      "height": "100%",
      "paddingTop": "16px",
      "paddingRight": "22px",
      "paddingBottom": "0px",
      "paddingLeft": "22px",
      "flexDirection": "column",
      "backgroundColor": "#ffffff"
    },
    ".top": {
      "minHeight": "20px",
      "alignItems": "center",
      "marginBottom": "24px"
    },
    ".text": {
      "fontFamily": "sys-sans-en",
      "fontWeight": "400",
      "fontSize": "14px",
      "color": "rgba(0,0,0,0.85)"
    },
    ".title": {
      "marginLeft": "6px"
    },
    ".icon": {
      "width": "20px",
      "height": "20px"
    },
    ".center": {
      "width": "100%",
      "height": "64px",
      "justifyContent": "space-between"
    },
    ".bigCard": {
      "width": "100%",
      "height": "64px",
      "justifyContent": "space-between"
    },
    ".image": {
      "width": "40px",
      "height": "40px"
    },
    ".message": {
      "flexDirection": "column",
      "justifyContent": "space-between",
      "alignItems": "center"
    },
    ".describe": {
      "fontWeight": "400",
      "fontSize": "14px",
      "color": "rgba(0,0,0,0.6)"
    },
    ".middleCard": {
      "display": "none",
      "width": "100%",
      "height": "64px",
      "justifyContent": "space-between"
    }
  },
  "apiVersion": {}
}