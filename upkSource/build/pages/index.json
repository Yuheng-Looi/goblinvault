{
  "template": [
    {
      "type": "card-template",
      "attr": {
        "category": "general",
        "entry": "notification"
      },
      "children": [
        {
          "type": "image",
          "attr": {
            "level": "0:A*",
            "src": "{{$r('images.logo')}}"
          }
        },
        {
          "type": "text",
          "attr": {
            "level": "1:B*",
            "value": "{{general_additional}}"
          }
        },
        {
          "type": "div",
          "attr": {
            "level": "5:F"
          },
          "children": [
            {
              "type": "div",
              "attr": {
                "level": "6-1:G1"
              },
              "children": [
                {
                  "type": "image",
                  "attr": {
                    "level": "0-1:A1*",
                    "src": "{{$r('images.logo')}}"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "2-1:C1",
                    "value": "{{general_serviceName}}"
                  }
                }
              ]
            },
            {
              "type": "div",
              "attr": {
                "level": "6-2:G2"
              },
              "children": [
                {
                  "type": "image",
                  "attr": {
                    "level": "3-5:D5",
                    "src": "{{$r('images.logo')}}"
                  }
                },
                {
                  "type": "progress",
                  "attr": {
                    "level": "3-4:D4",
                    "step": "2",
                    "color": "#FFCC24",
                    "bgColor": "#A4A4A4",
                    "percent": "{{general_percent}}"
                  }
                }
              ]
            },
            {
              "type": "text",
              "attr": {
                "level": "1-1:B1",
                "value": "{{general_additional}}"
              }
            },
            {
              "type": "text",
              "attr": {
                "level": "2-2:C2",
                "value": "{{general_title}}"
              }
            },
            {
              "type": "button",
              "attr": {
                "level": "4-3:E3",
                "type": "capsule",
                "value": "{{$t('strings.openApp')}}"
              }
            }
          ]
        }
      ]
    },
