  "data": {
    "percent": "60",
    "general_additional": "你好",
    "general_serviceName": "泛在服务",
    "general_title": "changchangde新标题",
    "title": "服务名称",
    "describe": "描述文本"
  },
  "actions": {
    "clickBtn": {
      "type": "deeplink",
      "uri": "nativeapp://android.intent.action.VIEW",
      "data": "",
      "params": {
        "travelId": "{{title}}",
        "trainNumber": "{{describe}}"
      }
    }
  },
  "styles": {
    ".container": {
      "width": "100%",
      "height": "100%",
      "paddingTop": "16px",
      "paddingRight": "22px",
      "paddingBottom": "0px",
      "paddingLeft": "22px",
      "flexDirection": "column"
    },
    ".top": {
      "minHeight": "20px",
      "alignItems": "center",
      "marginBottom": "24px"
    },
    ".text": {
      "fontFamily": "sys-sans-en",
      "fontWeight": "400",
      "fontSize": "14px",
      "color": "rgba(0,0,0,0.85)"
    },
    ".title": {
      "marginLeft": "6px"
    },
    ".icon": {
      "width": "20px",
      "height": "20px"
    },
    ".center": {
      "width": "100%",
      "height": "64px",
      "justifyContent": "space-between"
    },
    ".bigCard": {
      "width": "100%",
      "height": "64px",
      "justifyContent": "space-between"
    },
    ".image": {
      "width": "40px",
      "height": "40px"
    },
    ".message": {
      "flexDirection": "column",
      "justifyContent": "space-between",
      "alignItems": "center"
    },
    ".describe": {
      "fontWeight": "400",
      "fontSize": "14px",
      "color": "rgba(0,0,0,0.6)"
    },
    ".middleCard": {
      "display": "none",
      "width": "100%",
      "height": "64px",
      "justifyContent": "space-between"
    },
    "@MEDIA": [
      {
        "condition": "(aspect-ratio: 1*2)",
        ".container": {
          "paddingTop": "12px",
          "paddingRight": "24px",
          "paddingBottom": "0px",
          "paddingLeft": "24px"
        },
        ".top": {
          "display": "none"
        },
        ".image": {
          "width": "32px",
          "height": "32px"
        },
        ".bigCard": {
          "display": "none"
        },
        ".middleCard": {
          "display": "flex",
          "height": "53.1px"
        },
        ".describe": {
          "fontSize": "12px"
        }
      },
      {
        "condition": "(aspect-ratio: 2*2)",
        ".container": {
          "paddingTop": "16px",
          "paddingRight": "16px",
          "paddingBottom": "0px",
          "paddingLeft": "16px"
        },
        ".bigCard": {
          "display": "none"
        },
        ".middleCard": {
          "display": "flex"
        }
      }
    ]
  },
  "apiVersion": {}
}